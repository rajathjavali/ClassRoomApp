<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.rajathjavali.dbmsproject" >

    <supports-screens>
        android:smallScreens="true"
        android:normalScreens="true"
        android:largeScreens="true"
        android:xlargeScreens="true"
    </supports-screens>
    <!--
    android:resizeable="true"
    android:anyDensity="true"
    -->
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CALL_PHONE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/student"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".Login"
            android:label="@string/title_activity_login"
            android:windowSoftInputMode="adjustPan" >
        </activity>
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:windowSoftInputMode="adjustPan" >
        </activity>
        <activity
            android:name=".home"
            android:label="@string/title_activity_home"
            android:windowSoftInputMode="adjustPan" >

            <!-- android:parentActivityName=".Login" > -->
            <!-- <meta-data -->
            <!-- android:name="android.support.PARENT_ACTIVITY" -->
            <!-- android:value="com.example.rajathjavali.dbmsproject.Login" /> -->
        </activity>
        <activity
            android:name=".profile"
            android:label="@string/title_activity_profile"
            android:windowSoftInputMode="adjustPan" >
        </activity>
        <activity
            android:name=".ipadd"
            android:label="@string/title_activity_ipadd"
            android:windowSoftInputMode="adjustPan" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".pass"
            android:label="@string/title_activity_pass"
            android:windowSoftInputMode="adjustPan" >
        </activity>
        <activity
            android:name=".marks"
            android:label="@string/title_activity_marks"
            android:windowSoftInputMode="adjustPan" >
        </activity>
        <activity
            android:name=".marksdisp"
            android:label="@string/title_activity_marksdisp"
            android:windowSoftInputMode="adjustPan" >
        </activity>
        <activity
            android:name=".attenddisp"
            android:label="@string/title_activity_attenddisp"
            android:windowSoftInputMode="adjustPan" >
        </activity>
        <activity
            android:name=".contact"
            android:label="@string/title_activity_contact"
            android:windowSoftInputMode="adjustPan" >
        </activity>
        <activity
            android:name=".details"
            android:label="@string/title_activity_details"
            android:windowSoftInputMode="adjustPan" >
        </activity>
        <activity
            android:name=".notice"
            android:label="@string/title_activity_notice" >
        </activity>
    </application>

</manifest>
